{
    "name": "realtime-sync",
    "displayName": "Realtime Sync",
    "description": "Sync file edits between multiple people in realtime",
    "version": "0.0.1",
    "publisher": "Otter-Co",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Realtime Sync Configuration",
            "properties": {
                "realtime_sync.userName": {
                    "type": "string",
                    "default": false,
                    "description": "Username to use by default"
                },
                "realtime_sync.serverPort": {
                    "type": "integer",
                    "default": 8117,
                    "description": "Port to host server on"
                },
                "realtime_sync.clientPort": {
                    "type": "integer",
                    "default": 8117,
                    "description": "Port to connect to with client"
                }
            }
        },
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/uuid": "^3.4.3",
        "uuid": "^3.2.1"
    }
}
